def_1, def_2, new_1, new_2 = int(input()), int(input()), int(input()), int(input())
if new_1 - def_1 == -2 and new_2 - def_2 == 1 or new_1 - def_1 == -1 and new_2 - def_2 == 2 or new_1 - def_1 == 1 and new_2 - def_2 == 2 or new_1 - def_1 == 2 and new_2 - def_2 == 1 or new_1 - def_1 == 2 and new_2 - def_2 == -1 or new_1 - def_1 == 1 and new_2 - def_2 == -2 or new_1 - def_1 == -1 and new_2 - def_2 == -2 or new_1 - def_1 == -2 and new_2 - def_2 == -1:
    print('YES')
else :
    print('NO')
